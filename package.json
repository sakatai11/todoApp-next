{
  "name": "todoapp-next",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "rm -rf .next && next build",
    "start": "next start",
    "lint": "next lint --fix",
    "prettier": "prettier --write .",
    "format": "npm run prettier && npm run lint",
    "msw:init": "npx msw init public/ --save",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "docker:dev": "docker-compose up -d",
    "docker:test": "docker-compose -f docker-compose.test.yml up -d",
    "docker:test:down": "docker-compose -f docker-compose.test.yml down",
    "docker:test:run": "docker-compose -f docker-compose.test.yml up --build firebase-emulator-test nextjs-test integration-test",
    "emulator:start": "firebase emulators:start --only=firestore,auth,ui",
    "emulator:test": "firebase emulators:start --config firebase.test.json --only=firestore,auth,ui"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@fontsource/roboto": "^5.1.1",
    "@mui/icons-material": "^6.4.3",
    "@mui/material": "^6.4.3",
    "firebase-admin": "^13.2.0",
    "gray-matter": "^4.0.3",
    "next": "^15.2.4",
    "next-auth": "^5.0.0-beta.25",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-error-boundary": "^5.0.0",
    "remark": "^15.0.1",
    "remark-html": "^16.0.1",
    "reusify": "^1.0.4",
    "swr": "^2.3.3",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@eslint/eslintrc": "^3.2.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^14.3.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^22.13.1",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@typescript-eslint/eslint-plugin": "^8.24.0",
    "@typescript-eslint/parser": "^8.24.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^2.1.8",
    "@vitest/ui": "^2.1.8",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.20.0",
    "eslint-config-next": "^15.1.7",
    "eslint-config-prettier": "^10.0.1",
    "jsdom": "^24.0.0",
    "msw": "^2.8.7",
    "postcss": "^8.5.3",
    "prettier": "^3.5.0",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.3",
    "vite": "^5.4.11",
    "vitest": "^2.1.8"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}
